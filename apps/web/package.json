    "@next/mdx": "^13.4.7",
    "@storiny/proto": "*",
    "@storiny/ui": "*",
    "client-only": "^0.0.1",
    "little-state-machine": "^4.8.0",
    "nanoid": "^4.0.2",
    "next": "^13.4.7",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-virtuoso": "^4.3.8",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@mdx-js/loader": "^2.3.0",
    "@storiny/eslint-config": "*",
