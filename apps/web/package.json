{
  "name": "@storiny/web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "yarn minify && dotenv -e ../../.env.local next dev",
    "build": "yarn minify && dotenv -e ../../.env.production next build",
    "build:analyze": "yarn minify && cross-env ANALYZE=true dotenv -e ../../.env.production next build",
    "minify": "uglifyjs \"src/app/theme-sync.js\" --compress --mangle --output \"src/app/theme-sync.txt\"",
    "start": "next start",
    "lint": "next lint",
    "test": "jest"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.15",
    "@mdx-js/react": "^2.3.0",
