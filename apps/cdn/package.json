{
  "name": "@storiny/cdn",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e ../../.env.local docker compose up",
    "build": "rollup -c && docker compose build",
    "rollup": "rollup -c",
    "sync": "aws s3 sync buckets/base s3://base --delete --exclude 'sitemaps/*'",
    "sync:local": "cross-env AWS_ACCESS_KEY_ID=minio_user AWS_SECRET_ACCESS_KEY=minio_pass aws --endpoint-url=http://localhost:9000 s3 sync buckets/base s3://base --delete --exclude 'sitemaps/*'",
    "test": "rollup -c && jest",
    "clean": "rm -rf nginx/njs-files"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@storiny/eslint-config": "*",
    "@storiny/prettier-config": "*",
    "@storiny/test-utils": "*",
    "@storiny/tsconfig": "*",
    "babel-preset-njs": "^0.7.0",
    "cross-env": "^7.0.3",
    "docker-compose": "^0.24.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "njs-types": "^0.7.12",
    "prettier": "^2.8.8",
    "rollup": "^3.24.0",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.5"
  },
  "dependencies": {
    "@storiny/shared": "*"
  }
}
